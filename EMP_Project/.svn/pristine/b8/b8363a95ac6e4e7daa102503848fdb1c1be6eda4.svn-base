* GLOBAL:
   FORMAT               =  "%datetime - %level  [%fbase:%line; Module: %logger]:   %msg"
   FILENAME             =  "/var/log/asymtecUnloader.log"
   ENABLED              =  true
   TO_FILE              =  true
   TO_STANDARD_OUTPUT   =  true
   MILLISECONDS_WIDTH   =  6
   PERFORMANCE_TRACKING =  true
   MAX_LOG_FILE_SIZE    =  2097152 ## 2MB - Comment starts with two hashes (##)
   LOG_FLUSH_THRESHOLD  =  10 ## Flush after every 1 logs

* DEBUG:
   TO_STANDARD_OUTPUT   =  false
 
* TRACE:
   TO_STANDARD_OUTPUT   =  false
   