#include "pv/TypedPVMapping.h"

namespace GaGCppAPI {


    gc_DataType PVConverter<bool>::mType = gc_DATA_TYPE_UINT_8;
    gc_DataType PVConverter<int8_t>::mType = gc_DATA_TYPE_SINT_8;
    gc_DataType PVConverter<int16_t>::mType = gc_DATA_TYPE_SINT_16;
    gc_DataType PVConverter<int32_t>::mType = gc_DATA_TYPE_SINT_32;
    gc_DataType PVConverter<int64_t>::mType = gc_DATA_TYPE_SINT_64;
